$ORIGIN .
$TTL 3600       ; 1 hour
10.in-addr.arpa. IN      SOA     ns1.example.com. hostmaster.example.com. (
                        2007012501      ; serial number YYMMDDNN
                        28800           ; Refresh
                        7200            ; Retry
                        864000          ; Expire
                        86400           ; Min TTL
                        )

        IN      NS      ns1.example.com.
        IN      NS      ns2.example.com.

$ORIGIN 0.0.10.in-addr.arpa.
1          IN      PTR     foo.example.com.
